{
  "name": "@onimuxha/oxycons",
  "version": "1.0.8",
  "description": "A modern, comprehensive icon library for React applications with TypeScript support",
  "private": false,
  "license": "MIT",
  "author": "Endo Design",
  "repository": {
    "type": "git",
    "url": "https://github.com/endo-design/oxycons"
  },
  "homepage": "https://github.com/endo-design/oxycons",
  "bugs": {
    "url": "https://github.com/endo-design/oxycons/issues"
  },
  "keywords": [
    "icons",
    "react",
    "typescript",
    "svg",
    "icon-library",
    "ui-icons",
    "design-system"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/icons/index.js",
  "types": "./dist/icons/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/icons/index.js",
      "types": "./dist/icons/index.d.ts"
    },
    "./icons": {
      "import": "./dist/icons/icons.js",
      "types": "./dist/icons/icons.d.ts"
    },
    "./data": {
      "import": "./dist/icons/data.js",
      "types": "./dist/icons/data.d.ts"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "build:static": "next build && next export",
    "lib:build": "tsc -p tsconfig.lib.json",
    "prepublishOnly": "npm run lib:build",
    "generate:icons": "node scripts/generate-icons.js",
    "generate:icons:verbose": "node scripts/generate-icons.js --verbose",
    "update:indexes": "node scripts/update-indexes.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
